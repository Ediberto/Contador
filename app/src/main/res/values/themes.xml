<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Contador" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/pink_300</item>
        <item name="colorPrimaryVariant">@color/pink_500</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/brown_700</item>
        <item name="colorSecondaryVariant">@color/brown_900</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="TitloContador" parent="Widget.AppCompat.ActionBar.TabView">
        <item name = "android:fontFamily">@font/rubik_medium</item>
        <item name = "android:textColor">@color/brown_900</item>
        <item name = "android:textSize">18sp</item>
    </style>
    <style name="TitloFactory" parent="Widget.AppCompat.ActionBar.TabView">
        <item name = "android:fontFamily">@font/rubik_medium</item>
        <item name = "android:textColor">@color/pink_500</item>
        <item name = "android:textSize">18sp</item>
    </style>
    <style name="Titlo3" parent="Widget.AppCompat.ActionBar.TabView">
        <item name = "android:fontFamily">@font/rubik_medium</item>
        <item name = "android:textColor">@color/black</item>
        <item name = "android:textSize">18sp</item>
    </style>
    <style name="Contador" parent="Widget.AppCompat.ActionBar.TabView">
        <item name = "android:fontFamily">@font/rubik_medium</item>
        <item name = "android:textColor">@color/brown_700</item>
        <item name = "android:textSize">34sp</item>
    </style>
    <style name="Body1" parent="Widget.AppCompat.ActionBar.TabView">
        <item name = "android:fontFamily">@font/rubik_regular</item>
        <item name = "android:textSize">14sp</item>
    </style>
    <style name="ButtonPrimary" parent="Widget.AppCompat.Button">
        <item name = "android:fontFamily">@font/rubik_medium</item>
        <item name = "android:textColor">@color/brown_900</item>
    </style>
</resources>
